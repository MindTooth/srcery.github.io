{
  "author": "Srcery Team",
  "license": "MIT",
  "name": "srcery.github.io",
  "description": "Jekyll test site for srcery",
  "private": true,
  "devDependencies": {
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.2",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-nested": "^3.0.0",
    "postcss-preset-env": "^5.2.2",
    "postcss-scss": "^2.0.0",
    "prettier-standard": "^8.0.1",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "tailwindcss": "^0.6.3",
    "webpack": "^4.16.0",
    "webpack-command": "^0.4.1"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.3.5",
    "jquery": "^3.3.1",
    "jquery-lazy": "^1.7.9"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --run-dev --config=config/webpack.config.js",
    "jekyll-watch": "bundle exec jekyll serve --watch -V",
    "js-lint": "node_modules/standard/bin/cmd.js src/index.js",
    "js-pretty": "node_modules/prettier-standard/src/index.js src/index.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --run-prod --config=config/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --run-dev --watch --config=config/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/srcery-colors/srcery.git"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "Firefox ESR",
    "not ie <= 10",
    "not ie_mob <= 10",
    "not bb <= 10",
    "not op_mob <= 12.1"
  ]
}
